{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "./index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "./favicon.ico",
          "./index.html",
          "./manifest.webmanifest",
          "./*.css",
          "./*.js"
        ],
        "urls": [
          "https://www.djjb.de/images/logo.png"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "./assets/**",
          "./images/**",
          "./*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**",
    "!/**/*.rss"
  ],
  "navigationRequestStrategy": "freshness"
}
